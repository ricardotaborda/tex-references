@Article{Laurenzano_2010_BSSA,
  Title                    = {Effect of Vibrating Buildings on Free-Field Motion and on Adjacent Structures: {T}he {B}onefro ({I}taly) Case History},
  Author                   = {Laurenzano, Giovanna and Priolo, Enrico and Gallipoli, Maria Rosaria and Mucciarelli, Marco and Ponzo, Felice C.},
