@Article{Aki_2003_PEPI,
  Title                    = {A perspective on the history of Strong Motion Seismology},
  Author                   = {Aki, Keiiti},
  Journal                  = PEPI,
  Year                     = {2003},
  Number                   = {1-4},
  Pages                    = {5--11},
